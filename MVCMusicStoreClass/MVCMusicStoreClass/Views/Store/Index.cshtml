@model IEnumerable<MVCMusicStoreClass.Models.Genre>

@{
    ViewBag.Title = "Store"; }
<br />  <br /> <br /> <br /><br />
<h3>Browse Genres</h3>
<p> Select from @Model.Count() genres:</p>
<ul>
  
    @foreach (var genre in Model)
    {
        <li>@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
    }
</ul>