@model ComicBookGallery.Models.ComicBook[]


@{
    ViewBag.Title = "Comic Books";
}

<h2>@ViewBag.Title</h2>

<div class= "row">
    @foreach (var cbook in Model)
    {
        <div class="col-md-3">
            <h4>@cbook.DisplayText</h4>
            <img src="~/Images/@cbook.CoverImageFileName" alt="@cbook.DisplayText" class="img-responsive" />
        </div>
    }
  
</div>




