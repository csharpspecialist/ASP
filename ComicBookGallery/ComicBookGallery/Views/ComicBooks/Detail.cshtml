@{
    Layout = null;




}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details Page Dude</title>
</head>
<body>
    <div>
        <h1> @ViewBag.SeriesTitle </h1> <br />
        <h1> @ViewBag.Desc</h1> <br />
        <h1> Issue #      @ViewBag.IssNum </h1>

        <h1> Cost of Issue # @ViewBag.IssNum is @ViewBag.Pea</h1>
        @if (@ViewBag.Artists.Length > 0)
        {

            <h3>   Below are a List of contributing artists  </h3>
            <div>
                @foreach (var item in ViewBag.Artists)
                {
                    <h3>    <li>  @item  </li>   </h3>
                }

            </div>
      

            }

    </div>
</body>

</html>
